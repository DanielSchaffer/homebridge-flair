{
  "name": "homebridge-flair",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bassrock/homebridge-flair.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bassrock/homebridge-flair/issues"
  },
  "homepage": "https://github.com/bassrock/homebridge-flair#readme",
  "keywords": [
    "homebridge-flair",
    "flair"
  ],
  "engines": {
    "node": ">=6.0.0",
    "homebridge": ">=0.4.38"
  },
  "scripts": {
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --source-maps",
    "dropcache": "rm /usr/local/lib/node_modules/homebridge/node_modules/node-persist/storage/accessories/cachedAccessories",
    "lint": "eslint .",
    "prettier": "prettier --write src/**/*.js",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "start": "homebridge -D -P . -U ."
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  }
}
